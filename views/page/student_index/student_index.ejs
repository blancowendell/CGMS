<div class="container bootstrap snippets bootdeys">
    <div id="categories-container"></div>
</div>


<div class="modal fade" id="strandModal" data-bs-backdrop="static" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
      <div class="modal-content">
          <div class="modal-header" id="modal-header">
          <lord-icon src="https://cdn.lordicon.com/pdsourfn.json" stroke="bold"
          colors="success:#9cf4a7,secondary:#109121,tertiary:#ebfad1" class="db-hrmis"></lord-icon>
          <h3 class="modal-title" id="exampleModalLabel">View Strands</h3>
          <i class='bx bx-x' data-bs-dismiss="modal" aria-label="CLose"></i>
          </div>
          <div class="modal-body">
            <h4 id="modal-strand-name"></h4>
            <p><strong>Possible Jobs:</strong> <span id="modal-job-description"></span></p>
            <p><strong>Job Requirements:</strong> <span id="modal-job-requirements"></span></p>
            <p><strong>Skills Requirements:</strong> <span id="modal-skill-requirements"></span></p>
            <p><strong>Video Description:</strong> <span id="modal-video-description"></span></p>
            
            <video id="modal-video-player" controls style="width: 100%; display: none;">
              <source id="modal-video-source" type="video/mp4" />
              Your browser does not support the video tag.
            </video>
            
            <iframe id="modal-video-iframe" width="100%" height="315" style="display: none;" frameborder="0" allowfullscreen></iframe>
          </div>
          <div class="modal-footer" id="modal-footer">
          <button type="button" class="btn bg-gradient-secondary" data-bs-dismiss="modal">Close</button>
          </div>
      </div>
    </div>
</div>
  
