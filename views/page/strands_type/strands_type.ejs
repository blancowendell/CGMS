            <div class="col">
                <div class="form-floating">
                <input type="text" class="form-control border-form-modal" id="strandtype" name="strandtype" placeholder="First Name">
                <label for="floatingInput">Strands Type Name</label>
                </div>
            </div>
            <br>
            <div class="col">
              <div class="form-floating">
              <textarea class="form-control border-form-modal" id="description" name="description" placeholder="First Name"></textarea>
              <label for="floatingInput">Description</label>
              </div>
          </div>
          <br>
            </form>
        </div>
        <div class="modal-footer" id="modal-footer">
        <button type="button" class="btn bg-gradient-secondary" data-bs-dismiss="modal">Close</button>
        <button type="button" class="btn bg-gradient-success" id="addStrandsTypeBtn">Save</button>
